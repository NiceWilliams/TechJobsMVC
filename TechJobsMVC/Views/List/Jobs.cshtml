<div class="container body-content">

    <h1>@ViewBag.title</h1>

    @{
        //TODO #1: Use a loop to display job results in a table.
    }

    <table class="job-listing">
        @foreach(var job in ViewBag.jobs)
        {
            <tr>
                <td>
                    @job.Id<br>
                    @job.Name<br>
                    @job.Employer<br>
                    @job.Location<br>
                    @job.PositionType<br>
                    @job.CoreCompetency<br>
                </td>
            </tr>
        }
    </table>

</div>